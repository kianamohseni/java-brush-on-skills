package Inheritance;

public class zoo {

	public static void main(String[] args) {

		int weight;
		int height;
		
		weight = 1;
		height = 2;
		chicken chick1 = new chicken(weight, height);
		chick1.eat();
		chick1.fly();
		weight = 3;
		height = 4;
		eagle eagle1 = new eagle(weight, height);
		eagle1.eat();
		eagle1.fly();
	}

}
