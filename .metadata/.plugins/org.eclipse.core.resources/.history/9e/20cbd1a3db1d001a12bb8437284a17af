package java_basics;

public class animal {

	int age;
	String name;
	int weight;

	

public animal(int age, String name, int weightInPounds) {

	this.age = age;
	this.name = name;
	this.weight = weightInPounds;
	
}

public void walk() {
	System.out.println("walking ..." + this.age );
}

public static void fly(int arg) {
	int var;
	var = arg;
	System.out.println("fly over me" + var);
}
}

