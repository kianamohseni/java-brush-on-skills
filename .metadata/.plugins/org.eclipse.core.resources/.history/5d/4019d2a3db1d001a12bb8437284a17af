package java_basics;

public class humans {
	
	public int age;
	public int height;
	public String name;

	
	
	public humans(int age, int height, String name) {
		super();
		this.age = age;
		this.height = height;
		this.name = name;
	}



	public void speak(){
		System.out.println("age: " + age);
	}
		

}
